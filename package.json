{
  "name": "theme-dev",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && node scripts/migrateToRoot.js",
    "moveToObsidian": "node scripts/migrateToObsidian.js",
    "start": "concurrently 'npm run dev' 'npm run watch'",
    "watch": "watch 'npm run moveToObsidian' ./dist"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "sass": "^1.53.0",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "concurrently": "^7.3.0",
    "watch": "^1.0.2"
  }
}
